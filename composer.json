{
    "name": "curly-deni/permission-observer",
    "description": "This is my package permission-observer",
    "keywords": [
        "Danila Mikhalev",
        "laravel",
        "permissionobserver"
    ],
    "homepage": "https://github.com/curly-deni/permission-observer",
    "license": "MIT",
    "authors": [
        {
            "name": "Danila Mikhalev",
            "email": "danila@dan-mi.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/auth": "^12.10",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "illuminate/support": "^12.10",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "Aesis\\PermissionObserver\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Aesis\\PermissionObserver\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aesis\\PermissionObserver\\PermissionObserverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
